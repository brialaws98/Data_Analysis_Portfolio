## How does Race, Age, and Gender affect Job Title Salary? ##

SELECT
  Country,
      -- Will group Ages by age group
  CASE 
        WHEN age BETWEEN 20 AND 29 THEN '20s'
        WHEN age BETWEEN 30 AND 39 THEN '30s'
        WHEN age BETWEEN 40 AND 49 THEN '40s'
    END AS age_group,
  Gender,
  Salary

FROM `portfolio-project-397513.salary_by_job_title.Salary`

WHERE 
  Job_Title IN (
      'Customer Service Rep',
      'Customer Support Specialist',
      'Data Analyst',
      'Data Engineer',
      'Financial Analyst',
      'Front end Developer',
      'Mrketing Analyst',
      'Operations Analyst',
      'Project Manager',
      'Recruiter',
      'Web Developer'
  )

  GROUP BY 
    Country,
    age_group,
    Gender,
    Salary

-------------------------------------------------------------------------------
-------------------------------------------------------------------------------

## How does Years of Experience affect Salary in each Job Title? ##

SELECT 
  Years_of_Experience,
  Job_Title, 
  Salary

FROM `portfolio-project-397513.salary_by_job_title.Salary`

WHERE 
  Job_Title IN (
      'Customer Service Rep',
      'Customer Support Specialist',
      'Data Analyst',
      'Data Engineer',
      'Financial Analyst',
      'Front end Developer',
      'Mrketing Analyst',
      'Operations Analyst',
      'Project Manager',
      'Recruiter',
      'Web Developer'
  )
    ANd Salary >= 35000

  ORDER BY 
    Years_of_Experience,
    Salary
    ASC
---------------------------------------------------------
---------------------------------------------------------
## Does Gender affect Salary?

SELECT 
  Gender,
  Salary

FROM `portfolio-project-397513.salary_by_job_title.Salary`

WHERE 
  Job_Title IN (
      'Customer Service Rep',
      'Customer Support Specialist',
      'Data Analyst',
      'Data Engineer',
      'Financial Analyst',
      'Front end Developer',
      'Mrketing Analyst',
      'Operations Analyst',
      'Project Manager',
      'Recruiter',
      'Web Developer'
  )

-------------------------------------------
-------------------------------------------
## Which Gender is involved more in Data Analysis?

SELECT 
  Gender,
  Job_Title

FROM `portfolio-project-397513.salary_by_job_title.Salary`

WHERE 
  Job_Title = 'Data Analyst'
  
--------------------------------------------------------------------
